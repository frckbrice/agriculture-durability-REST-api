{
  "name": "agriculture-durability-rest-api",
  "version": "0.0.2",
  "description": " Agriculture durability REST API is a backend application built using NestJS framework that provides RESTful API endpoints for managing and analyzing agricultural durability data. The application allows users to perform CRUD operations on durability records, as well as retrieve insights and statistics related to agricultural durability.",
  "author": "Avom brice",
  "private": true,
  "license": "LICENSED",
  "scripts": {
    "build": "prisma generate && nest build",
    "vercel-build": "prisma generate && prisma migrate deploy && nest build",
    "postinstall": "prisma generate",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:https": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/config": "^3.3.0",
    "@nestjs/event-emitter": "^2.1.1",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.4.22",
    "@nestjs/core": "^10.4.22",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.4.22",
    "@nestjs/throttler": "^6.5.0",
    "@prisma/client": "^5.22.0",
    "@types/lodash": "^4.17.23",
    "axios": "^1.13.5",
    "async": "^3.2.6",
    "cache-manager": "^5.7.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "lodash": "^4.17.23",
    "moment-timezone": "^0.5.48",
    "nestjs-fetch": "^0.1.0",
    "nodemailer": "^6.10.1",
    "prisma": "^5.22.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.2",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@nestjs/axios": "^3.1.3",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.22",
    "@types/cron": "^2.4.0",
    "@types/express": "^4.17.25",
    "@types/jest": "^29.5.2",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^20.19.33",
    "@types/nodemailer": "^6.4.15",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "form-data": "^4.0.5",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}